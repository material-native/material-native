{
  "name": "material-native",
  "version": "0.0.0-alpha.13",
  "description": "Material Design components for React Native",
  "license": "MIT",
  "keywords": [
    "react",
    "react-component",
    "react-native",
    "material",
    "material design",
    "android",
    "ios"
  ],
  "author": "Redwerks (http://redwerks.org)",
  "contributors": [
    "Daniel Friesen <d@danf.ca> (http://danf.ca)",
    "Yingxin Wu <yingxinwu.g@gmail.com>",
    "Quentin Valmori (https://github.com/Crash--)",
    "Alexander Price (https://github.com/alexprice91)"
  ],
  "homepage": "https://github.com/material-native/material-native",
  "bugs": "https://github.com/material-native/material-native/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/material-native/material-native.git"
  },
  "main": "src/index.js",
  "files": [
    "src",
    ".eslintrc.json"
  ],
  "scripts": {
    "start": "node ./node_modules/react-native/local-cli/cli.js start --config ./rn-cli.config.js",
    "explorer-ios": "react-native run-ios",
    "explorer-android": "cd examples/MaterialExplorer && react-native run-android",
    "precommit": "lint-staged",
    "test": "npm run jest && npm run lint",
    "jest": "jest",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "color-composite": "^0.1.0",
    "color-parse": "^1.3.2",
    "create-react-class": "^15.6.0",
    "hoist-non-react-methods": "^1.0.2",
    "prop-types": "^15.5.10",
    "wcag-contrast": "^0.1.0",
    "zen-observable": "^0.6.0"
  },
  "peerDependencies": {
    "react-native": ">= 0.40",
    "react-native-vector-icons": ">= 4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-preset-react-native": "^4.0.0",
    "cheerio": "^1.0.0-rc.1",
    "eslint": "^4.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.1.0",
    "gulp": "^3.9.1",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.3",
    "lodash": "^4.17.4",
    "react": "16.0.0-alpha.6",
    "react-element-to-jsx-string": "^12.0.0",
    "react-native": "^0.43.4",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-test-renderer": "16.0.0-alpha.6",
    "request": "^2.81.0"
  },
  "jest": {
    "notify": true,
    "preset": "react-native",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "modulePathIgnorePatterns": [
      "examples"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  }
}
